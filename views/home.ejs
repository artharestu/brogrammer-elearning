<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%- include('_bootstrap') %>
    <title>Document</title>
</head>

<body class="bg-light">
  <%- include('_nav') %>
    <div class="container">
      <% dataCourses.forEach(element=> { %>
        <div class="card" style="width: 18rem;">
          <img src="/gambar/<%= element.thumbnail %>" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">
              <%= element.name %>
            </h5>
            <p class="card-text">
              <%= element.description %>
            </p>
            <p class="card-text">
              <%= element.price %>
            </p>
            <a href="#" class="btn btn-primary">Subscribe</a>
          </div>
        </div>
        <% }) %>
    </div>
</body>

</html>